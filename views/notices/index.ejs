<% layout('/index') -%>
<section id="sl_notice_index">
	<article class="table-responsive">	
		<table width="100%" cellpadding="0" cellspacing="0" class="table table-striped">
			<colgroup>
				<col />
				<col />
				<col />
				<col />
			</colgroup>
			<thead>
				<tr>
					<th class="sl_t_id hidden-sm hidden-xs"><%=gettext('label_id') %></th>		
					<th class="sl_t_title"><%=gettext('label_title') %></th>
					<th class="sl_t_count"><%=gettext('label_count') %></th>
					<th class="sl_t_created_at"><%=gettext('label_created_at') %></th>
				</tr>
			</thead>
			<tbody>
   <% if(data.length){ %>                                
   <% for(var i = 0;i < data.length;i++) { %>                 
                <tr>
                    <td><%=data[i].id%></td>
                    <td><%=data[i].title%></td>
                    <td><%=data[i].count %></td>
                    <td><%=data[i].created_at %></td>
                    <td>                    
                    </td>
                </tr>
            <% } %>
      <% }else{ %>
                 <tr>
                    <td colspan="4">No user</td>
                 </tr>
            <% } %>	
			</tbody>
		</table>
	</article>
	<div id="sl_index_bottom_menu">
	</div>
</section>
